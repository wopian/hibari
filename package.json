{
  "name": "hibari",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "James Harris <wopian@wopian.me>",
  "private": true,
  "scripts": {
    "test": "jest --coverage",
    "dev": "node build/dev-server.js",
    "build": "node build/build.js",
    "lint": "eslint --ext .js,.vue src",
    "log:major": "changelog -M",
    "log:minor": "changelog -m",
    "log:patch": "changelog -p",
    "webpack-defaults": "webpack-defaults"
  },
  "dependencies": {
    "buefy": "~0.6.1",
    "client-oauth2": "~4.1.0",
    "kitsu": "~3.1.0",
    "vue": "~2.5.2",
    "vue-i18n": "~7.3.2",
    "vue-meta": "~1.3.1",
    "vue-router": "~3.0.1",
    "vuex": "~3.0.0",
    "vuex-persist": "~0.6.0"
  },
  "devDependencies": {
    "autoprefixer": "~7.1.5",
    "babel-core": "~6.26.0",
    "babel-eslint": "~8.0.1",
    "babel-loader": "~7.1.2",
    "babel-plugin-istanbul": "~4.1.5",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-env": "~1.6.1",
    "babel-preset-stage-0": "~6.24.1",
    "babel-register": "~6.26.0",
    "chalk": "~2.3.0",
    "connect-history-api-fallback": "~1.5.0",
    "copy-webpack-plugin": "~4.2.0",
    "css-loader": "~0.28.7",
    "cssnano": "~3.10.0",
    "eslint": "~4.11.0",
    "eslint-config-standard": "~10.2.1",
    "eslint-friendly-formatter": "~3.0.0",
    "eslint-loader": "~1.9.0",
    "eslint-plugin-html": "~4.0.0",
    "eslint-plugin-import": "~2.8.0",
    "eslint-plugin-node": "~5.2.0",
    "eslint-plugin-promise": "~3.6.0",
    "eslint-plugin-standard": "~3.0.1",
    "eventsource-polyfill": "~0.9.6",
    "express": "~4.16.2",
    "extract-text-webpack-plugin": "~3.0.1",
    "file-loader": "~1.1.5",
    "friendly-errors-webpack-plugin": "~1.6.1",
    "generate-changelog": "~1.6.0",
    "html-webpack-plugin": "~2.30.1",
    "http-proxy-middleware": "~0.17.4",
    "jest": "~21.2.1",
    "jest-serializer-vue": "~0.3.0",
    "node-sass": "~4.7.1",
    "optimize-css-assets-webpack-plugin": "~3.2.0",
    "progress-bar-webpack-plugin": "~1.10.0",
    "pug": "~2.0.0-rc.4",
    "pug-loader": "~2.3.0",
    "rimraf": "~2.6.2",
    "sass-loader": "~6.0.6",
    "semver": "~5.4.1",
    "shelljs": "~0.7.8",
    "sw-precache-webpack-plugin": "~0.11.4",
    "uglifyjs-webpack-plugin": "~1.1.0",
    "url-loader": "~0.6.2",
    "vue-jest": "~1.1.0",
    "vue-loader": "~13.5.0",
    "vue-server-renderer": "~2.5.6",
    "vue-style-loader": "~3.0.3",
    "vue-template-compiler": "~2.5.2",
    "vue-test-utils": "~1.0.0-beta.5",
    "webpack": "~3.8.1",
    "webpack-bundle-size-analyzer": "~2.7.0",
    "webpack-dev-middleware": "~1.12.0",
    "webpack-hot-middleware": "~2.20.0",
    "webpack-merge": "~4.1.0",
    "webpack-pwa-manifest": "~3.3.2"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0",
    "yarn": ">= 1.0.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 ChromeAndroid versions",
    "last 2 Edge versions",
    "last 2 Firefox versions",
    "last 2 FirefoxAndroid versions",
    "last 2 Safari versions",
    "last 2 iOS versions"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,vue}"
    ],
    "coveragePathIgnorePatterns": [
      "src/(locale|api|store|router|main.js)"
    ],
    "coverageDirectory": "<rootDir>/test/coverage",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "mapCoverage": true,
    "moduleNameMapper": {
      "=/([^\\.]*)$": "<rootDir>/src/components/$1.vue",
      "@/([^\\.]*)$": "<rootDir>/src/$1.vue"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "globals": {
      "NODE_ENV": "test"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "baseBranch": "new",
    "assignees": [
      "wopian"
    ],
    "pinVersions": false,
    "automerge": false,
    "commitMessage": "{{semanticPrefix}}update {{depName}} to {{newVersion}}",
    "labels": [
      "dependencies"
    ],
    "lockFileMaintenance": {
      "enabled": false
    },
    "semanticCommits": true,
    "separateMajorReleases": false,
    "unpublishSafe": true,
    "schedule": [
      "before 5am on monday"
    ],
    "prCreation": "not-pending",
    "prTitle": "{{semanticPrefix}}{{#if isPin}}pin{{else}}update{{/if}} {{depName}} to {{#if isRange}}{{newVersion}}{{else}}{{#if isMajor}}{{newVersionMajor}}.x{{else}}{{newVersion}}{{/if}}{{/if}}",
    "packageRules": [
      {
        "packagePatterns": [
          "^eslint"
        ],
        "groupName": "eslint"
      },
      {
        "packagePatterns": [
          "^webpack"
        ],
        "groupName": "webpack"
      },
      {
        "packagePatterns": [
          "^babel"
        ],
        "groupName": "babel"
      },
      {
        "packagePatterns": [
          "^jest"
        ],
        "groupName": "jest"
      },
      {
        "packagePatterns": [
          "^vue"
        ],
        "groupName": "vue"
      },
      {
        "packagePatterns": [
          "^vuex"
        ],
        "groupName": "vuex"
      }
    ]
  }
}
